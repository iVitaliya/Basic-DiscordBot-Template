{
    "opts": {
        "template": "./node_modules/@alexispuga/jsdoc-template",
        "destination": "docs",
        "recurse": true
    },
    "source": {
        "include": "src"
    },
    "templates": {
        "custom": {
		"siteURL": "https://github.com/AlexisPuga/jsdoc-template",
		tags": {
			"body": [
				"<style>#npm-badge img{display:table}#demo-advice{padding:20px;text-align:center;background:#06060c;color:white}#demo-advice a{color:inherit;text-decoration:underline}</style>",
				"<div id='demo-advice'><b>Discord Bot Example</b>: All content was generated with examples taken from the <a href='https://jsdoc.app' rel='noopener nofollow'>JSDoc page.</a></div>"
			]
		}
	}
    }
}
